<h1>Listing default_line_calendars</h1>

<table>
  <thead>
    <tr>
      <th>Fecha</th>
      <th>Nombre</th>
      <th>Status</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @default_line_calendars.each do |default_line_calendar| %>
      <tr>
        <td><%= default_line_calendar.fecha %></td>
        <td><%= default_line_calendar.nombre %></td>
        <td><%= default_line_calendar.status %></td>
        <td><%= link_to 'Show', default_line_calendar %></td>
        <td><%= link_to 'Edit', edit_default_line_calendar_path(default_line_calendar) %></td>
        <td><%= link_to 'Destroy', default_line_calendar, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Default line calendar', new_default_line_calendar_path %>
