<h1>Listing default_calendars</h1>

<table>
  <thead>
    <tr>
      <th>Anio</th>
      <th>Status</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @default_calendars.each do |default_calendar| %>
      <tr>
        <td><%= default_calendar.anio %></td>
        <td><%= default_calendar.status %></td>
        <td><%= link_to 'Show', default_calendar %></td>
        <td><%= link_to 'Edit', edit_default_calendar_path(default_calendar) %></td>
        <td><%= link_to 'Destroy', default_calendar, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Default calendar', new_default_calendar_path %>
